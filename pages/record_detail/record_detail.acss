  text-overflow: ellipsis;
  white-space: nowrap;
}
.r_t_r{
  position: absolute;
  top: 20rpx;
  right: 0;
  font-size: 24rpx;
  line-height: 26rpx;
}
.r_con{
  height: 140rpx;
  padding-bottom: 20rpx;
