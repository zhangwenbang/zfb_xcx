page{
  background: #f7f7f7;
}

.h_info{
  height:98rpx;
  line-height: 98rpx;
  padding: 0 30rpx;
  background: #fafafa;
}
.h_left{
  float: left;
  font-size: 32rpx;
  line-height: 98rpx;
  color: #ff7e2d;
}

.h_right{
  float: right;
