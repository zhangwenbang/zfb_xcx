  height: 140rpx;
  position: relative;
}
.r_c_f01{
  padding-left: 26rpx;
  padding-top: 30rpx;
}
.r_f01_l{
  float: left;
  font-size: 24rpx;
  line-break: 32rpx;
  width: 300rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.r_f01_r{
  float: right;
  font-size: 24rpx;
